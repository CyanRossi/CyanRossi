 import pandas as pd 

 import numpy as np 

 s1=pd.Series([10,20,30],index=['a','b','c']) 

 s2=pd.Series([2,5], index=['a','b']) 

 print(s1) 

 print(s2) 

 print('---------------------------------------------------------------------') 

 print(s1+s2) 

 print('----------------------------------------------------------------------') 

 print(s1-s2) 

 print('-------------------------------------------------------------------------') 

 print(s1*s2) 

 print('---------------------------------------------------------------------------') 

 print(s1/s2) 

 print('--------------------------------------------------------------------------') 

 print(s2/s1) 

  

