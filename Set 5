 (QUESTION 2) 

  

 import matplotlib.pyplot as plt 

  

 States = ["DL", "TN", "AP", "KA"] 

  

 Players = [5,11,10,7] 

  

 plt.title ("Bar Chart") 

  

 plt.xlabel ("States") 

  

 plt.ylabel ("Players") 

  

 plt.bar (States, Players, width=0.25, color = ['red', 'b','g', 'black']) 

  

 plt.show () 

